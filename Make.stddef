# "Make.stddef"
# make-independent definitions

SHELL = /bin/sh
AR = ar
ARFLAGS = -ru
# add -v for verbose
AWK = awk
CAT = cat
CPP = cpp
CVS = cvs
DIFF = diff -B
# -B: ignore blank line diffs
ECHO = echo
GREP = grep
SED = sed
LN = ln -fs
RANLIB = ranlib
RM = rm -f
MV = mv -f
SYNC = sync
TOUCH = touch
TAR = tar -czvf

# defined with intention of self-modification...
THISMAKEFILE = Makefile


