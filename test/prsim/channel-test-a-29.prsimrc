source channel-test-a-01.prsimrc
channel-show-all

channel-source-args X 0 0 0 1
channel-source-args-loop Y 0 2 0 1
channel-sink Z

channel-watchall
channel-reset-all

set X.e 1
set Y.e 1
set Z.d[0] 0
set Z.d[1] 1
