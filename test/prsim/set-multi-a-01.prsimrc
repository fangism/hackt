# "set-multi-a-01.prsimrc"
# @haco@ set-multi-a.haco-c

watchall
# getall .

get a.q[0].p[0].x[0..3]

get a.q[0].p[1..2].x[3]

set a.q[0].p[0].x[0..3] 1
cycle
status-newline 1

set a.q[0].p[1..2].x[3] 0
cycle
status-newline 0
status-newline 1

set a.q[0].p[0..1].x[2..3] 0
cycle
status-newline 0
status-newline 1

