# "channel-test-b-03.prsimrc"
# @haco@ channel-test-b.haco-c
# @deps@ channel-test-a-values-X.txt
# testing source/sinks on different channels

source channel-test-b-01.prsimrc

channel-source-loop L channel-test-a-values-X.txt
channel-sink R
channel-show L
channel-show R
channel-watchall

channel-reset-all
channel-show L
channel-show R
cycle
channel-show L
channel-show R
assert L.d[0] 0
assert L.d[1] 0
assert L.e 1
assert R.d[0] 0
assert R.d[1] 0
assert R.e 0

channel-release-all
queue

step 150
assert-queue
echo could just keep going, source is looping

echo backtrace should be able to find cycle through sources and sinks!
backtrace L.e

