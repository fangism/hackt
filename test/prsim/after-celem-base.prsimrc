# testing different delays on rules

# this script should run silently by itself
# This is called by other scripts

# exercising ~a & ~b
set a 0
set c 0
set b 0
set d 1
cycle

# exercising c & d
set b 1
cycle
set c 1
cycle

# exercising ~c & ~d
set c 0
set d 0
cycle

# exercising a & b
set c 1
cycle
set a 1
cycle

