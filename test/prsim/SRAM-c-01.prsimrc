# "SRAM-c-01.prsimrc"
# @haco@ SRAM-c.haco-c

# echo-commands on
timing after
weak-rules on
fanin bl
fanin _bl
watchall
watchall-queue
unstable warn
unstable-dequeue

set _pchg 0
set wl[1] 0
set wv 0
set wl[0] 0
set we 0
cycle
status U
time
echo done precharging, now writing...

set _pchg 1
cycle
time

echo enabled write, set zero value, now setting wordline
echo may see momentary X on _bl because we_ is in flight
set wv 0
set we 1
advance 30
set wl[0] 1
cycle
time

fanin-get c[0].r.t
fanin-get c[0].r.f

fanin-get bl
fanin-get _bl

# getall c[0]
# getall c[1]

unsetall
queue
echo queue should be empty after unsetall

