# "channel-test-b-15.prsimrc"
# copy-m0odified from "channel-test-b-02.prsimrc"
# @haco@ channel-test-b.haco-a
# testing source/sinks on different channels

source channel-test-b-01.prsimrc

channel-source-args L 0 0 1 1 0 1 0 1
channel-sink R
channel-show L
channel-show R
channel-watchall

channel-reset-all
channel-show L
channel-show R
cycle
channel-show L
channel-show R
assert L.d[0] 0
assert L.d[1] 0
assert L.e 1
assert R.d[0] 0
assert R.d[1] 0
assert R.e 0

channel-release-all
queue

cycle
echo source has run out of values!

echo deadlock analysis should point back to source
why-not L.e

