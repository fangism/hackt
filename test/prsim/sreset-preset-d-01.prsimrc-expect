	0	_sReset : 0
	0	_pReset : 0
	10	_out : 1	[by _pReset:=0]
	20	out : 0	[by _out:=1]
	20	in : 1
	20	_pReset : 1
would be state-holding here 
	20	_sReset : 1
	30	_out : 0	[by _sReset:=1]
	40	out : 1	[by _out:=0]
	40	_sReset : 0
	40	in : 0
	50	_out : 1	[by in:=0]
	60	out : 0	[by _out:=1]
	60	_pReset : 0
	60	_pReset : 1
would be state-holding here 
	60	in : 1
	60	in : 0
	60	_sReset : 1
	60	in : 1
	70	_out : 0	[by in:=1]
	80	out : 1	[by _out:=0]
	80	in : 0
	90	_out : 1	[by in:=0]
	100	out : 0	[by _out:=1]
