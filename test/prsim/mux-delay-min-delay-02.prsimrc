set x[0] 0
set x[1] 0
set x[2] 0
set x[3] 0
cycle

echo ----------------
set s[0][0] X
set s[0][1] X
set s[1][0] X
set s[1][1] X
set x[0] X
set x[1] X
set x[2] X
set x[3] X
cycle
echo ----------------

