# testing setting and resetting of random seed

seed48 0 0 0
echo initial seed should be 0s
seed48

timing random
echo step a few events in random timing
set a 0
cycle

echo make sure reset actually resets the seed
reset
# keep same checkpoint name as before
autosave on
seed48

