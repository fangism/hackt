# "pchb-test-a-02.prsimrc"
# @haco@ pchb-test-a.haco-c
# @hacprsim-flags@ -f precharge-invariants

channel L e:1 :0 d:2
channel R e:1 :0 d:2
channel-source-args-loop L 0 1 1 0
channel-expect-args-loop R 0 1 1 0
channel-sink R

# watchall
mode reset
timing random
set _Reset 0
channel-reset-all
cycle
no-status X

mode fatal
set _Reset 1
channel-release-all
step-event 100000

echo some time later...
# getall b

assert-queue
no-status X
echo passed QDI test

