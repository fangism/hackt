# "xor-test-a-01.prsimrc"
# @haco@ xor-test-a.haco-c
# don't need hacprsim-flags -f fast-weak-keepers

# weak-rules on
keeper-check-fail fatal
unstable-dequeue
unstable warn

watchall
set a 0
set b 0
cycle

set a 1
set b 1
cycle

set a 0
set b 0
cycle

set b 1
set a 1
cycle

set b 0
set a 0
cycle

set b 1
cycle

set a 1
set b 0
cycle

set a 0
set b 1
cycle

set b 0
set a 1
cycle

set b 1
set a 0
cycle

