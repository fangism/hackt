/**
import "../oscillator.hac";
**/

template <pint N>
defproc ring_oscillator(bool x[N]; bool _Reset) {
prs {
	(:i:N-1:
		x[i]		=> x[i+1]-
	)
	x[N-1] & _Reset		=> x[0]-
}
}

bool _Reset;
ring_oscillator<13> R( , _Reset);


