# "channel-test-b-random-03.prsimrc"
# copy-modified from "channel-test-b-random-01.prsimrc"
# @haco@ channel-test-b.haco-c
# testing source/sinks on different channels

source channel-test-b-01.prsimrc

echo sourcing X denotes random value
channel-source-args-loop L X
channel-sink R
channel-show L
channel-show R
channel-watchall

channel-reset-all
channel-show L
channel-show R
cycle
channel-show L
channel-show R
assert L.d[0] 0
assert L.d[1] 0
assert L.e 1
assert R.d[0] 0
assert R.d[1] 0
assert R.e 0

channel-release-all
queue

step 333
assert-queue

