# "channel-test-a-05.prsimrc"
# copy-modified from "channel-test-a-04.prsimrc"
# @haco@ channel-test-a.haco-a
# @deps@ channel-test-a-values-X.txt
# testing channel source
# different initialization sequence

# echo-commands on
source channel-test-a-01.prsimrc

channel-show X
getall X
watchall
channel-source X channel-test-a-values-X.txt
channel-watch X
channel-reset X
cycle
getall X
channel-show X

echo manually bucketing, channel source is stopped
set X.e 1
cycle
assert X.d[0] 0
assert X.d[1] 0

echo releasing channel X (source)
channel-release X
cycle
echo should be data yet because ack is already on

echo -----------------------------------
assert X.d[0] 1
assert X.d[1] 0
# we know the first data value
channel-show X
backtrace X.d[0]

echo - - - - - - - - - - - - - - - - - -
set X.e 0
cycle
channel-show X

echo -----------------------------------
set X.e 1
cycle
assert X.d[0] 0
assert X.d[1] 1
# we know the second data value
channel-show X
# backtrace X.d[0]

echo - - - - - - - - - - - - - - - - - -
set X.e 0
cycle
channel-show X

echo -----------------------------------
set X.e 1
cycle
channel-show X

echo - - - - - - - - - - - - - - - - - -
set X.e 0
cycle
channel-show X

echo -----------------------------------
set X.e 1
cycle
channel-show X

echo - - - - - - - - - - - - - - - - - -
set X.e 0
cycle
channel-show X

echo -----------------------------------
set X.e 1
cycle
channel-show X

echo - - - - - - - - - - - - - - - - - -
set X.e 0
cycle
channel-show X

echo -----------------------------------
set X.e 1
cycle
channel-show X

echo - - - - - - - - - - - - - - - - - -
set X.e 0
cycle
channel-show X

echo -----------------------------------
set X.e 1
cycle
channel-show X

echo - - - - - - - - - - - - - - - - - -
set X.e 0
cycle
channel-show X

echo -----------------------------------
set X.e 1
cycle
channel-show X

echo - - - - - - - - - - - - - - - - - -
set X.e 0
cycle
channel-show X

echo -----------------------------------
set X.e 1
cycle
channel-show X

echo - - - - - - - - - - - - - - - - - -
set X.e 0
cycle
channel-show X

echo -----------------------------------
set X.e 1
cycle
channel-show X

echo - - - - - - - - - - - - - - - - - -
set X.e 0
cycle
channel-show X

echo -----------------------------------
echo no more values!!!
set X.e 1
cycle
channel-show X
getall X
assert X.d[0] 0
assert X.d[1] 0

