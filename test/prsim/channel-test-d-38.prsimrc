# "channel-test-d-38.prsimrc"
# @haco@ channel-test-d.haco-c

# testing hexadecimal values
source channel-test-d-01.prsimrc
echo no values given: will reset to neutral
!touch channel-test-d-38-empty.txt
channel-source-file-loop Y channel-test-d-38-empty.txt
channel-show-all
# channel-sink Y
echo this time, no sink

channel-watch Y

echo resetting all channels...
channel-reset-all
watchall
cycle

echo releasing all channels...
channel-release-all
cycle

assertn-queue
echo all values went to X because Y.e is X

channel-show Y
echo setting Y.e -> 0, resets data rails
set Y.e 0
cycle

channel-show Y
echo setting Y.e -> X
set Y.e X
cycle

channel-show Y
echo setting Y.e -> 1, also causes data rails to reset
set Y.e 1
cycle

channel-show Y
echo setting Y.e -> X
set Y.e X
cycle

!rm -f channel-test-d-38-empty.txt
