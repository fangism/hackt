# "channel-ledr-a-02.prsimrc"
# @haco@ channel-ledr-a.haco-c

source channel-ledr-a-01.prsimrc

watchall
echo freezing C
channel-stop C
set C.d 0
set C.r 0
cycle
echo thawing C
channel-release C
channel-watch C
cycle

echo repeat 3 values
set C.r 1
cycle
set C.r 0
cycle
set C.r 1
cycle

echo toggle 3 values
set C.d 1
cycle
set C.d 0
cycle
set C.d 1
cycle

echo repeat 3 values
set C.r 0
cycle
set C.r 1
cycle
set C.r 0
cycle

echo toggle 3 values
set C.d 0
cycle
set C.d 1
cycle
set C.d 0
cycle

