# echo-commands on
watchall
set X.clk 0
set X.d 0
cycle

channel-show X
channel-reset-all
cycle
channel-show X
channel-release-all
channel-show X
cycle

set X.clk 1
c
set X.d 1
set X.clk 0
c
set X.clk 1
c
set X.clk 0
set X.d 0
c
set X.clk 1
c
set X.clk 0
set X.d 1
c
set X.clk 1
c

