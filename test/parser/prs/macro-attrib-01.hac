// test for macro attributes

template <pint SIZE>
defproc foo(bool t, f; bool in[2], out)
{
  prs {
    [keeper=0] passp<20*SIZE>(t,in[0],out)
    [keeper=0] passn<10*SIZE>(f,in[0],out)
    [keeper=0] passp<20*SIZE>(f,in[1],out)
    [keeper=0] passn<10*SIZE>(t,in[1],out)
  }
}

