"foo.x[0].a" -> "foo.x[0].b"-
~"foo.x[0].a" -> "foo.x[0].b"+
"foo.y" -> "foo.x[1].b"-
~"foo.y" -> "foo.x[1].b"+
"foo.x[2].a" -> "foo.z"-
~"foo.x[2].a" -> "foo.z"+
"foo.x[3].a" -> "foo.x[3].b"-
~"foo.x[3].a" -> "foo.x[3].b"+
@ "foo.x[0].a" iscomb
@ "foo.y" iscomb !autokeeper
@ "foo.x[2].a" iscomb
@ "foo.z" !autokeeper
@ "foo.x[3].a" iscomb
connect "foo.x[0].a" "foo.p"
connect "foo.x[0].b" "foo.q"
connect "foo.y" "foo.x[1].a"
connect "foo.z" "foo.x[2].b"
