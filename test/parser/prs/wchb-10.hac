// "wchb-10.hac", copy-modified from "wchb-08.hac"
// a reduced test case

defchan e1of2 <: chan(bool) (bool d[2], e, t, f) { d[0] = f; d[1] = t; }
// defproc e1of2 (bool d[2], e, t, f) { d[0] = f; d[1] = t; }

defproc wchb (e1of2? l; e1of2! r) {
l?;	// locally consumed
r!;	// locally produced
prs {
(:d:2:
	l.d[d] => r.d[d]+
)
	r.e => l.e+
}
}

wchb W;

