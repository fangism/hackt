"p" -> "q"-
~"p" -> "q"+
"x[1].a" -> "x[1].b"-
~"x[1].a" -> "x[1].b"+
"x[2].a" -> "x[2].b"-
~"x[2].a" -> "x[2].b"+
@ "p" iscomb
@ "x[2].b" iscomb
connect "!GND" "x[0].!GND"
connect "!Vdd" "x[0].!Vdd"
connect "p" "x[0].a"
connect "q" "x[0].b"
connect "!GND" "x[1].!GND"
connect "!Vdd" "x[1].!Vdd"
connect "!GND" "x[2].!GND"
connect "!Vdd" "x[2].!Vdd"
