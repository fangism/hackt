defproc wrap(bool w, x, y, z) {
bool j, k, l;
inv P(w, x);
x @ [iscomb];
inv Q(j, k);
y @ [iscomb];
inv R(k, l);
l @ [iscomb];
}

bool a, b, c, d;
wrap present;
present.z = a;
present.y = b;
c = present.x;
d = present.w;


