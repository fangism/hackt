"foo.x[0].a" -> "foo.x[0].b"-
~"foo.x[0].a" -> "foo.x[0].b"+
"foo.y" -> "foo.x[1].b"-
~"foo.y" -> "foo.x[1].b"+
"foo.x[2].a" -> "foo.z"-
~"foo.x[2].a" -> "foo.z"+
@ "foo.x[0].a" iscomb
@ "foo.y" !autokeeper
@ "foo.z" !autokeeper
connect "foo.x[0].a" "foo.p"
connect "foo.x[0].b" "foo.q"
connect "foo.y" "foo.x[1].a"
connect "foo.z" "foo.x[2].b"
connect "foo.x[0].a" "foo.x[3].a"
connect "foo.x[0].b" "foo.x[3].b"
connect "foo.x[2].a" "foo.x[4].a"
connect "foo.z" "foo.x[4].b"
connect "foo.y" "foo.x[5].a"
connect "foo.x[1].b" "foo.x[5].b"
