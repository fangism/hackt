// "031.in"
// testing for prime numbers
// should pass

template <pint P>
defproc prime_array(bool x[P+1]) {
bool composite;
x[0] = x[1] = composite;
(;i:P:
[ i*i <= P ->
	(;j:2..i:		// all factors (even composites)
	(;k:2..P/j:
		x[j*k] = composite;
	)
	)
]
)
}

// will reveal primes up to 100
prime_array<100> p;

