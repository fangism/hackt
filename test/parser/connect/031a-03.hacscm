(use-modules (hackt hackt))

(define refs (collect-reference-subinstances (parse-raw-reference "load")))

; splitting out by type and index
(map reference-type refs)
(map reference-index refs)

(filter (lambda (r) (= process-tag (reference-type r))) refs)
(filter (lambda (r) (= int-tag (reference-type r))) refs)
(filter (lambda (r) (= bool-tag (reference-type r))) refs)

