	$(TOUCH) $@

-include createfailtest.autodepend

.hacktcreatefailtest:
	@$(DUMMY_TEST_SCRIPT)


# unroll and create have been fused together
if USE_UNROLL_PHASE
.haco-u.haco-c:
else
.haco.haco-c:
endif
