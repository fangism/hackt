// "subckt-02.hac"
// copy-modified from "subckt-01.hac"
// test for behavior of netgen w.r.t. subcircuits
// @hacknet_flags@ -p 'inv_c<>'
// expect fail: netgen only allows one-level of subckt

defproc inv_c(bool a, b) {
bool _x, x, _y;
prs {
subckt<"first"> {
	a	=> _x-
	_x	=> x-
	
subckt<"second"> {
	x	=> _y-
	_y	=> b-
}
}
}
}

inv_c foo;

