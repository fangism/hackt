# @haco@ wchb-token-ring-c.haco-a
# testing stability of token ring

source initial-reset.prsimrc
set Reset 0
set _Reset 1

channel L e:1 :0 d:2
channel M e:1 :0 d:2
channel R e:1 :0 d:2
channel-watch L

channel-show-all

timing random

echo expect to see on channel L: [1,0]*
step 380
assert-queue


