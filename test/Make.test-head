# "Make.test-head"
# include this first in every subdirectory's Makefile.am
#	$Id: Make.test-head,v 1.18 2006/07/28 01:03:18 fang Exp $
#	vim: ft=automake

include $(top_srcdir)/Make.stddef

# suffixes used by all test directories' Makefiles
# automake will convert these into .SUFFIXES:
SUFFIXES = .hac .hacktcmpltest .hacktobjtest .hacktunrolltest \
	.hacktcreatetest .hacktalloctest .hacktcflattest .hacktsprstest \
	.hacktcflatseutest \
	.depend \
	.haco .haco-u .haco-c .haco-a .prs .sprs .prsseu \
	.prsimexpr .prsimexpr-dot .prsimexpr-ps \
	.prsimexpr-O1 .prsimexpr-dot-O1 \
	.haco-dump .haco-u-dump .haco-c-dump .haco-a-dump \
	.outdump .indump .unrolldump .createdump .allocdump \
	.haco-c-from-haco .haco-a-from-haco .haco-a-from-u \
	.hacktcreatefromhacotest .hacktallocfromhacotest \
	.prsimrc .prsimtest .prsimckpttest .prsimopttest \
	.hacktprsdottest

if HAVE_CFLAT
SUFFIXES += .cast .castprs
endif

# for compile include paths
HACO_FLAGS =

# the {in,out,unroll,create,alloc}dump suffixes are just 
# for testing convenience.   They follow the procedures in the test scripts.  


# names of test cases (without extension) whose compile error output
# differs when using bison vs. yacc (result of different state-machines)
BISON_OUTPUTS =

# tests expecting empty .prs outputs
EMPTY_CFLAT_PRS_TESTS =

