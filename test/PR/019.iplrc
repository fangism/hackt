# "019.iplrc"
# tetrahedron example

object-type size=<10,10,10>
repeat 4 add-object type=1
place 0 <18,20,-15>
place 1 <35,40,-10>
place 2 <20,45,5>
place 3 <40,22,10>
add-channel 0 1
add-channel 0 2
add-channel 0 3
add-channel 1 2
add-channel 1 3
add-channel 2 3
add-z-well 0.0
add-x-wells 0 5 100
add-y-wells 0 5 100

parameter time_step=0.01
parameter damping=0.5
parameter repulsion_coeff=20

