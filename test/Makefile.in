clean-test:
	@$(ECHO) "#!$(SHELL)" > $@
	@$(ECHO) "$(RM) $(TEST_REPORT)" >> $@
	@$(CHMOD) +x $@

# This needs to be specified in Makefile.am to take effect
# SUFFIXES = .artctest .in

.in.stderr:
	@$(TOUCH) $@

subdir.not_a_test:
	@$(ECHO) "#!$(SHELL)" > $@
	@$(ECHO) "echo \"In $(subdir):\" >> $(TEST_REPORT)" >> $@
	@$(CHMOD) a+x $@

.in.artctest:
	@$(ECHO) "#!$(SHELL)" > $@
	@$(ECHO) "# \"$@\"" >> $@
	@$(ECHO) "$(ARTC_TEST_SH) $(ARTC_EXE) $(srcdir) $* \"$(STATE_ENUM_FILTER)\" $(TEST_REPORT)" >> $@
	@$(CHMOD) a+x $@

all-local: subdir.not_a_test $(TESTS)

# automatically removes $(CLEANFILES)
clean-local:
	$(RM) *.core core.*

# some stderr files don't exist, because empty output files are optional
# this shouldn't really be necessary...
# dist-hook-extra:
#	$(RM) $(distdir)/subdir.not_a_test

dist-hook-missing: $(EXTRA_DIST)
	@for f in $(EXTRA_DIST); do \
		if ! [ -f $$f ] ; then \
			$(TOUCH) $$f ; \
		fi ; \
	done

# remove extraneous auto-generated files
dist-hook:
	rm -rf `find $(distdir) -name subdir.not_a_test`
