// testing printing to file, and flushing

chp {
	fprint("lib-fflush-test-a-out.txt",
		"welcome to libc!\n");
	fprint("lib-fflush-test-a-out.txt",
		28, " is a perfect number.\n");
	fprint("lib-fflush-test-a-out.txt",
		"To thine own self be ", true, ".\n");
	fflush("lib-fflush-test-a-out.txt")
}

