#!@SH_PATH@
# "test-series.sh.in"
# convenience script for running a sequence of tests on one input file.
#	$Id: test-series.sh.in,v 1.2.2.1 2005/08/07 01:07:29 fang Exp $
# This file originally came from "test-series.sh" in revision prehistory.  

# usage: pass in the root names of the test cases (without the .in extension)

for t in $@
do
	if ! [ -f $t.artctest ]
	then
		@CALLMAKE@ $t.artctest
	fi
	@ECHO@ $t.artctest
	$t.artctest
	if ! [ -s $t.test ]
	then
		if ! [ -f $t.artobjtest ]
		then
			@CALLMAKE@ $t.artobjtest
		fi
		@ECHO@ $t.artobjtest
		$t.artobjtest
		if ! [ -f $t.artobjunrolltest ]
		then
			@CALLMAKE@ $t.artobjunrolltest
		fi
		@ECHO@ $t.artobjunrolltest
		$t.artobjunrolltest
		if [ -f $t.artobjunroll ]
		then

			if ! [ -f $t.hacktcreatetest ]
			then
				@CALLMAKE@ $t.hacktcreatetest
			fi
			@ECHO@ $t.hacktcreatetest
			$t.hacktcreatetest
		fi
	fi
done


