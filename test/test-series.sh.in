#!@SH_PATH@
# "test-series.sh.in"
# convenience script for running a sequence of tests on one input file.
#	$Id: test-series.sh.in,v 1.5 2005/12/08 22:01:15 fang Exp $
# This file originally came from "test-series.sh" in revision prehistory.  

# usage: pass in the root names of the test cases (without the .in extension)

for t in $@
do
	if test ! -f $t.artctest
	then
		@CALLMAKE@ $t.artctest
	fi
	@ECHO@ $t.artctest
	$t.artctest
	if test ! -s $t.test
	then
		if test ! -f $t.artobjtest
		then
			@CALLMAKE@ $t.artobjtest
		fi
		@ECHO@ $t.artobjtest
		$t.artobjtest
		if test -s $t.artobj
		then
			if test ! -f $t.artobjunrolltest
			then
				@CALLMAKE@ $t.artobjunrolltest
			fi
			@ECHO@ $t.artobjunrolltest
			$t.artobjunrolltest
		fi
		if test -s $t.artobjunroll
		then
			if test ! -f $t.hacktcreatetest
			then
				@CALLMAKE@ $t.hacktcreatetest
			fi
			@ECHO@ $t.hacktcreatetest
			$t.hacktcreatetest
		fi
		if test -s $t.hacktobjcreate
		then
			if test ! -f $t.hacktalloctest
			then
				@CALLMAKE@ $t.hacktalloctest
			fi
			@ECHO@ $t.hacktalloctest
			$t.hacktalloctest
		fi
	fi
done


