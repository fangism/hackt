#!@SH_PATH@
# "test-series.sh.in"
# convenience script for running a sequence of tests on one input file.
#	$Id: test-series.sh.in,v 1.4 2005/09/14 15:30:38 fang Exp $
# This file originally came from "test-series.sh" in revision prehistory.  

# usage: pass in the root names of the test cases (without the .in extension)

for t in $@
do
	if ! [ -f $t.artctest ]
	then
		@CALLMAKE@ $t.artctest
	fi
	@ECHO@ $t.artctest
	$t.artctest
	if ! [ -s $t.test ]
	then
		if ! [ -f $t.artobjtest ]
		then
			@CALLMAKE@ $t.artobjtest
		fi
		@ECHO@ $t.artobjtest
		$t.artobjtest
		if [ -s $t.artobj ]
		then
			if ! [ -f $t.artobjunrolltest ]
			then
				@CALLMAKE@ $t.artobjunrolltest
			fi
			@ECHO@ $t.artobjunrolltest
			$t.artobjunrolltest
		fi
		if [ -s $t.artobjunroll ]
		then
			if ! [ -f $t.hacktcreatetest ]
			then
				@CALLMAKE@ $t.hacktcreatetest
			fi
			@ECHO@ $t.hacktcreatetest
			$t.hacktcreatetest
		fi
		if [ -s $t.hacktobjcreate ]
		then
			if ! [ -f $t.hacktalloctest ]
			then
				@CALLMAKE@ $t.hacktalloctest
			fi
			@ECHO@ $t.hacktalloctest
			$t.hacktalloctest
		fi
	fi
done


